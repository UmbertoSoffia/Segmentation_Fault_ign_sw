<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:foreground="@color/cardview_shadow_start_color"
    tools:context=".Reservation">


  <Spinner
      android:id="@+id/spinner_num_people"
      android:layout_width="198dp"
      android:layout_height="48dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="@+id/spinner_struct"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="@+id/spinner_struct"
      app:layout_constraintTop_toBottomOf="@+id/spinner_struct"
      app:layout_constraintVertical_bias="0.12" />

  <TextView
      android:id="@+id/textView20"
      android:layout_width="99dp"
      android:layout_height="51dp"
      android:layout_marginTop="128dp"
      android:text="Numero Partecipanti"
      android:textAlignment="center"
      android:textAllCaps="false"
      android:textColor="#424242"
      app:layout_constraintBottom_toTopOf="@+id/cancel_button_reservation"
      app:layout_constraintEnd_toStartOf="@+id/spinner_date"
      app:layout_constraintHorizontal_bias="0.53"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView11"
      app:layout_constraintVertical_bias="0.102" />

  <Spinner
      android:id="@+id/spinner_struct"
      android:layout_width="198dp"
      android:layout_height="48dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.924"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.212"
      tools:ignore="SpeakableTextPresentCheck" />

  <Spinner
      android:id="@+id/spinner_date"
      android:layout_width="198dp"
      android:layout_height="48dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="@+id/spinner_struct"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="@+id/spinner_struct"
      app:layout_constraintTop_toBottomOf="@+id/spinner_struct"
      app:layout_constraintVertical_bias="0.357" />

  <Spinner
      android:id="@+id/attività"
      android:layout_width="215dp"
      android:layout_height="49dp"
      android:layout_marginStart="52dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.555"
      app:layout_constraintStart_toEndOf="@+id/textView11"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.074" />


  <TextView
      android:id="@+id/textView11"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Attività"
      android:textColor="#424242"
      app:layout_constraintBottom_toTopOf="@+id/textView8"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.184"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.544" />

  <TextView
      android:id="@+id/textView8"
      android:layout_width="68dp"
      android:layout_height="26dp"
      android:layout_marginStart="68dp"
      android:text="Struttura"
      android:textColor="#424242"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toStartOf="@+id/spinner_struct"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.219" />

  <TextView
      android:id="@+id/textView9"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="128dp"
      android:text="Data_Ora"
      android:textColor="#424242"
      app:layout_constraintBottom_toTopOf="@+id/cancel_button_reservation"
      app:layout_constraintEnd_toStartOf="@+id/spinner_date"
      app:layout_constraintHorizontal_bias="0.482"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView11"
      app:layout_constraintVertical_bias="0.465" />

  <Button
      android:id="@+id/confirm_reservation"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:backgroundTint="#4CAF50"
      android:text="@string/conferma"
      android:textColor="#000000"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.743"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.841" />

  <Button
      android:id="@+id/cancel_button_reservation"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="92dp"
      android:backgroundTint="#BA1414"
      android:text="@string/annulla"
      android:textColor="#000000"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toStartOf="@+id/confirm_reservation"

      app:layout_constraintHorizontal_bias="1.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.841" />

</androidx.constraintlayout.widget.ConstraintLayout>